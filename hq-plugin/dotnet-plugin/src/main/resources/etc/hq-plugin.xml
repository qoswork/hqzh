    <server name=".NET"
            version="1.1"
            platforms="Win32">

        <plugin type="measurement"
                class="DotNetMeasurementPlugin"/>

        <plugin type="autoinventory"
                class="DotNetDetector"/>

        <metrics>
            <include name="dotnet-avail"/>
            <include name="clr-memory"/>
            <include name="clr-loading"/>
            <include name="clr-ex"/>
            <include name="clr-jit"/>
            <include name="clr-threads"/>
            <include name="clr-security"/>
            <include name="clr-remote"/>
            <!-- server only -->
            <include name="clr-net"/>
            <include name="clr-data"/>
        </metrics>
        
        <!-- http://msdn.microsoft.com/en-us/library/ms254503.aspx -->
        <service name="Data Provider for SqlServer">
        	<property name="pdhDP" value="pdhSQLDP"/>    		        
            <metrics>            	
            	<include name="database-data-provider"/>
            	<include name="database-data-provider-sql-server"/>
           	</metrics>
        </service>

        <service name="Oracle Data Provider for .Net">
		   	<property name="pdhDP" value="pdhOracleDP"/>            
            <metrics>            	            	
            	<include name="database-data-provider"/>
           	</metrics>
        </service>

        <service name="Application">
            <metrics>
                <include name="dotnet-avail"/>
                <include name="clr-memory"/>
                <include name="clr-loading"/>
                <include name="clr-ex"/>
                <include name="clr-jit"/>
                <include name="clr-threads"/>
                <include name="clr-security"/>
                <include name="clr-remote"/>
            </metrics>

            <config>
                <option name="app.name"
                        description="Application name"
                        default="PerfBrowser"/>
            </config>
        </service>

        <service name="ASP.NET App">
            <config>
                <option name="app.name" description="Application name"/>
                <option name="app.path" description="Application path"/>
            </config>

            <filter name="template" value="pdh:%app.path%(%app.name%):${name}"/>
            <metric name="Availability" alias="Availability" template="pdh:%app.path%(%app.name%)\Anonymous Requests:${alias}" indicator="true" category="AVAILABILITY"/>
            <metric name="Anonymous Requests"/>
            <metric name="Anonymous Requests/Sec" indicator="true"/>
            <metric name="Cache Total Entries"/>
            <metric name="Cache Total Turnover Rate"/>
            <metric name="Cache Total Hits" collectionType="trendsup"/>
            <metric name="Cache Total Misses" collectionType="trendsup"/>
            <metric name="Cache Total Hit Ratio" indicator="true"/>
            <metric name="Cache API Entries"/>
            <metric name="Cache API Turnover Rate"/>
            <metric name="Cache API Hits" collectionType="trendsup"/>
            <metric name="Cache API Misses" collectionType="trendsup"/>
            <metric name="Cache API Hit Ratio" indicator="true"/>
            <metric name="Output Cache Entries"/>
            <metric name="Output Cache Turnover Rate"/>
            <metric name="Output Cache Hits" collectionType="trendsup"/>
            <metric name="Output Cache Misses" collectionType="trendsup"/>
            <metric name="Output Cache Hit Ratio" indicator="true"/>
            <metric name="Compilations Total" collectionType="trendsup"/>
            <metric name="Debugging Requests" collectionType="trendsup"/>
            <metric name="Errors During Preprocessing" collectionType="trendsup"/>
            <metric name="Errors During Compilation" collectionType="trendsup"/>
            <metric name="Errors During Execution" collectionType="trendsup"/>
            <metric name="Errors Unhandled During Execution"/>
            <metric name="Errors Unhandled During Execution/Sec"/>
            <metric name="Errors Total"/>
            <metric name="Errors Total/Sec" indicator="true"/>
            <metric name="Pipeline Instance Count"/>
            <metric name="Request Bytes In Total" collectionType="trendsup" units="B"/>
            <metric name="Request Bytes Out Total" collectionType="trendsup" units="B"/>
            <metric name="Requests Executing"/>
            <metric name="Requests Failed" collectionType="trendsup"/>
            <metric name="Requests Not Found" collectionType="trendsup"/>
            <metric name="Requests Not Authorized" collectionType="trendsup"/>
            <metric name="Requests In Application Queue" indicator="true"/>
            <metric name="Requests Timed Out" collectionType="trendsup"/>
            <metric name="Requests Succeeded" collectionType="trendsup"/>
            <metric name="Requests Total" collectionType="trendsup"/>
            <metric name="Requests/Sec" indicator="true"/>
            <metric name="Sessions Active" indicator="true"/>
            <metric name="Sessions Abandoned" collectionType="trendsup"/>
            <metric name="Sessions Timed Out" collectionType="trendsup"/>
            <metric name="Sessions Total"/>
            <metric name="Transactions Aborted" collectionType="trendsup"/>
            <metric name="Transactions Committed" collectionType="trendsup"/>
            <metric name="Transactions Pending"/>
            <metric name="Transactions Total"/>
