for id in `./hqapi1-3.1/bin/hqapi.sh resource list   --prototype="Linux" --user $HQ_USER --password $HQ_PASSWORD --host $CLONE_SERVERIP --port $CLONE_SERVER_PORT|grep "<Resource "|grep "vmc-ssrc-rh136"|cut -d'=' -f2|cut -d' ' -f1|cut -d'"' -f2`; do ./hqapi1-3.1/bin/hqapi.sh resource delete --id=$id --user $HQ_USER --password $HQ_PASSWORD --host $CLONE_SERVERIP --port $CLONE_SERVER_PORT; done
