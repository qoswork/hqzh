                category="THROUGHPUT"
                group="Throughput"
                collectionType="trendsup"
                units="B"/>

        <metric name="Bytes Sent"
                alias="BytesSent"
                template="${net.domain}:Platform=Win32:Bytes Sent"
                category="THROUGHPUT"
